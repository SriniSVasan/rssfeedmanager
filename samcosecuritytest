package com.samco.rssfeed.model;

import java.util.ArrayList;
import java.util.Arrays;

public class FeedRssChannel {
	
   String title;
   String linkUrl;
   String description;
   String language;
   ArrayList<RssItem> rssItem;
public String getTitle() {
	return title;
}
public void setTitle(String title) {
	this.title = title;
}
public String getLinkUrl() {
	return linkUrl;
}
public void setLinkUrl(String linkUrl) {
	this.linkUrl = linkUrl;
}
public String getDescription() {
	return description;
}
public void setDescription(String description) {
	this.description = description;
}
public String getLanguage() {
	return language;
}
public void setLanguage(String language) {
	this.language = language;
}
public ArrayList<RssItem> getRssItem() {
	return rssItem;
}
public void setRssItem(ArrayList<RssItem> rssItem) {
	this.rssItem = rssItem;
}
@Override
public String toString() {
	return "FeedRssChannel [title=" + title + ", linkUrl=" + linkUrl + ", description=" + description + ", language="
			+ language + ", rssItem=" + rssItem + "]";
}


}

///////


package com.samco.rssfeed.model;

public class RssItem {
	
	@Override
	public String toString() {
		return "RssItem [title=" + title + ", linkUrl=" + linkUrl + ", guidUrl=" + guidUrl + ", description="
				+ description + ", imageTitle=" + imageTitle + ", pubDate=" + pubDate + "]";
	}
	String title;
	String linkUrl;
	String guidUrl;
	String description;
	RSSItemImageTitle imageTitle;
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getLinkUrl() {
		return linkUrl;
	}
	public void setLinkUrl(String linkUrl) {
		this.linkUrl = linkUrl;
	}
	public String getGuidUrl() {
		return guidUrl;
	}
	public void setGuidUrl(String guidUrl) {
		this.guidUrl = guidUrl;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public RSSItemImageTitle getImageTitle() {
		return imageTitle;
	}
	public void setImageTitle(RSSItemImageTitle imageTitle) {
		this.imageTitle = imageTitle;
	}
	public String getPubDate() {
		return pubDate;
	}
	public void setPubDate(String pubDate) {
		this.pubDate = pubDate;
	}
	String pubDate;
	

}


/////////////

package com.samco.rssfeed.model;

public class RSSItemImageTitle {
	
	String title;
	String linkUrl;
	String srcUrl;
	String pubDate;
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getLinkUrl() {
		return linkUrl;
	}
	public void setLinkUrl(String linkUrl) {
		this.linkUrl = linkUrl;
	}
	@Override
	public String toString() {
		return "RSSItemImageTitle [title=" + title + ", linkUrl=" + linkUrl + ", srcUrl=" + srcUrl + ", pubDate="
				+ pubDate + "]";
	}
	public String getSrcUrl() {
		return srcUrl;
	}
	public void setSrcUrl(String srcUrl) {
		this.srcUrl = srcUrl;
	}
	public String getPubDate() {
		return pubDate;
	}
	public void setPubDate(String pubDate) {
		this.pubDate = pubDate;
	}

}

