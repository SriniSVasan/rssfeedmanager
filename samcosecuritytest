package com.samco.rssfeed.controller;
import java.util.*;

import com.samco.rssfeed.service.FeedRSSParser;
import com.samco.rssfeed.user.RssFeedUser;
public class FeedRssChannelController {
	
	public static void main(String args[]) 
	{
		ArrayList<RssFeedUser> users=new ArrayList<>();
		RssFeedUser rssuser=new RssFeedUser();
		Scanner sc=new Scanner(System.in);  
		System.out.print("Enter Name ");  
		rssuser.setName(sc.next());  
		System.out.print("Enter Password [enter first four letter of name followed by dob year(fourdigit) ");  
		rssuser.setPwd(sc.next()); 
		System.out.print("Enter DOB (dd/mm/yyyy) format "); 
		rssuser.setDob(sc.next());
		
		FeedRSSParser parser =new FeedRSSParser();
		parser.showDetailsByUser(rssuser.getName());
		
	}

}

