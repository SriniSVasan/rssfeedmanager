package com.samco.rssfeed.service;
//package de.vogella.rss.read;

import java.io.FileInputStream;
import java.io.IOException;
import java.io.InputStream;
import java.io.StringReader;
import java.net.MalformedURLException;
import java.net.URISyntaxException;
import java.net.URL;
import java.util.ArrayList;
import java.util.Enumeration;

import javax.xml.stream.XMLEventReader;
import javax.xml.stream.XMLInputFactory;
import javax.xml.stream.XMLStreamException; 
import javax.xml.stream.events.Characters;
import javax.xml.stream.events.StartElement;
import javax.xml.stream.events.XMLEvent;


import javax.xml.namespace.QName;
import javax.xml.stream.events.Attribute;


import com.samco.rssfeed.model.FeedRssChannel;
import com.samco.rssfeed.model.RSSItemImageTitle;
import com.samco.rssfeed.model.RssItem;




public class FeedRSSParser {
//    static final String TITLE = "title";
//    static final String DESCRIPTION = "description";
//    static final String CHANNEL = "channel";
//    static final String LANGUAGE = "language";
//    static final String COPYRIGHT = "copyright";
//    static final String LINK = "link";
//    static final String AUTHOR = "author";
//    static final String ITEM = "item";
//    static final String PUB_DATE = "pubDate";
//    static final String GUID = "guid";

    //final URL url;
    StringReader reader;
    String path;
    
    public FeedRSSParser() {
    	
    }

    public FeedRSSParser(String path) {
        try {
        	this.path=path;
         
            
        } catch (Exception e) {
            throw new RuntimeException(e);
        }
    }

    
    public FeedRssChannel readFeed() throws XMLStreamException {
    	
    	  XMLInputFactory inputFactory = XMLInputFactory.newInstance();
          // Setup a new eventReader
          
    	  InputStream in = read();
         int i=0;
 
          XMLEventReader eventReader = null;
		try {
			eventReader = inputFactory.createXMLEventReader(in);
		} catch (XMLStreamException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
          FeedRssChannel channel=new FeedRssChannel();
          RssItem itm=null;
          RSSItemImageTitle imgTitle=null;
          boolean title=false,link=false,description=false,items=false;
          ArrayList<RssItem> itms=new ArrayList<RssItem>();
    	while (eventReader.hasNext()) {
    	    XMLEvent nextEvent = null;
			try {
				nextEvent = eventReader.nextEvent();
				
			} catch (XMLStreamException e) {
			//	System.out.println("exectoin "+e);
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
    	    if (nextEvent!=null && nextEvent.isStartElement()) {
    	        StartElement startElement = nextEvent.asStartElement();    	 
    	        String val=startElement.getName().getLocalPart();
    	      //  System.out.println("startelement "+val);
    	        
    	        switch (val) {    	       
    	        case "channel":
    	        	break;
    	            case "title":   
    	            	if (!items)
    	            	{
    	            	 nextEvent = eventReader.nextEvent();
    	        //    	 System.out.println(nextEvent);
    	            	 if (nextEvent.asCharacters().getData().trim().length()==0)
    	            		 nextEvent = eventReader.nextEvent();
    	            	 if (channel.getTitle()==null)
    	            	channel.setTitle(nextEvent.asCharacters().getData());   
    	            	}
    	            	else
    	            	{
    	            	 nextEvent = eventReader.nextEvent();
    	            	 nextEvent = eventReader.nextEvent();
    	          //  	  System.out.println("item title "+nextEvent);	
    	            	 if (itm!=null)
    	            	 {
    	            		 itm.setTitle(nextEvent.asCharacters().getData());
    	            	 }
   						
    	            	}
    	                break;
    	            case "link":
    	            	if (!items)
    	            	{
    	                nextEvent = eventReader.nextEvent();
    	            //    System.out.println(nextEvent);
    	                channel.setLinkUrl(nextEvent.asCharacters().getData()); 
    	            	}
    	            	else
    	            	{
    	            		 nextEvent = eventReader.nextEvent();
      					//	 System.out.println("item link "+nextEvent);
      						 if (itm!=null)
        	            	 {
        	            		 itm.setLinkUrl(nextEvent.asCharacters().getData());
        	            	 }
    	            	}
    	                break;
    	            case "description":    	 
    	            	if (!items)
    	            	{
    	                nextEvent = eventReader.nextEvent();
    	                if (nextEvent.asCharacters().getData().trim().length()==0)
   	            		 nextEvent = eventReader.nextEvent();
    	               // System.out.println(nextEvent);
    	                if (channel.getDescription()==null)
        	            	channel.setDescription(nextEvent.asCharacters().getData());  
    	            	}
    	            	else
    	            	{
    	            		 nextEvent = eventReader.nextEvent();
    	            		 nextEvent = eventReader.nextEvent();
      					//	 System.out.println("item description "+nextEvent);	
      						 if (itm!=null)
        	            	 {
        	            		 itm.setDescription(nextEvent.asCharacters().getData());  
        	            	 }
    	            		
    	            	}
    	                break; 	            
    	            
    	            case "language":
    	                nextEvent = eventReader.nextEvent();
    	                //System.out.println(nextEvent);
    	                channel.setLanguage(nextEvent.asCharacters().getData());    	               
    	                break;
    	                
    	            case "item":
    	            	 if (itm!=null)
    	            	 {
    	            		 itms.add(itm);
    	            	 }
    	            	 itm=new RssItem();
    	            	 items=true;
				         break;	   
    	         
					case "guid":
						nextEvent = eventReader.nextEvent();
						//System.out.println("guid "+nextEvent);	
						itm.setGuidUrl(nextEvent.asCharacters().getData());
						break;
						
					case "image":
						imgTitle=new RSSItemImageTitle();
						//nextEvent = eventReader.nextEvent();
						//  StartElement element = event.asStartElement();
					//	  startElement.getAttributeByName(null)
						//System.out.println("image "+nextEvent);	
						 Attribute tit = startElement.getAttributeByName(new QName("title"));
						 imgTitle.setTitle(tit.getValue());
						 Attribute lik = startElement.getAttributeByName(new QName("link"));
						 imgTitle.setLinkUrl(lik.getValue());
						 Attribute srcs = startElement.getAttributeByName(new QName("src"));
						 imgTitle.setSrcUrl(srcs.getValue());
						 if (itm!=null)
						 {
							 itm.setImageTitle(imgTitle);
						 }
						 
						 
	                        break;
						//itm.setGuidUrl(nextEvent.asCharacters().getData());
							
					
					case "pubDate":
						  nextEvent = eventReader.nextEvent();
						  //System.out.println("pubdate1 "+nextEvent);						 
						itm.setPubDate(nextEvent.asCharacters().getData());
						break;   
    
    	        }
    	    }
    	    if (nextEvent!=null && nextEvent.isEndElement()) {
//    	        EndElement endElement = nextEvent.asEndElement();
//    	        if (endElement.getName().getLocalPart().equals("website")) {
//    	            websites.add(website);
//    	        }
    	    }
    	   // itms.add(itm);
    	}
    	channel.setRssItem(itms);
    	//System.out.println(channel);
    	return channel;
    }
    
    
   


    private InputStream read() {
    	 FileInputStream fin=null;
        try {
         // InputStream in =new FileInputStream(new File());
        	System.out.println(this.path+"rssfeed.xml");
           fin=new FileInputStream(this.path+"rssfeed.xml");    
        	//  return url.openStream();
        } catch (IOException e) {
            throw new RuntimeException(e);
        }
        return fin;
    }
    
    public void showDetailsByUser(String username)
    {
     ClassLoader classLoader = Thread.currentThread().getContextClassLoader();
   	 URL resource =classLoader.getResource("resources");
   	// System.out.println("original "+resource.getPath().toString());
   	 this.path=resource.getPath().toString();//.substring(6);
  // 	 System.out.println(this.path);
   	// FeedRSSParser parser =new FeedRSSParser(path);
   	 

      try {
		FeedRssChannel channel=readFeed();
		System.out.println("please use the follow urls to access the news");
   	    channel.getRssItem().stream().map(p->p.getLinkUrl()).forEach(System.out::println);
   	   
	} catch (XMLStreamException e) {
		// TODO Auto-generated catch block
		e.printStackTrace();
	}
      
    }
    public static void main(String[] args) throws URISyntaxException, IOException {
        
//    	 ClassLoader classLoader = Thread.currentThread().getContextClassLoader();
//    	 URL resource =classLoader.getResource("resources");
//    	 System.out.println("original "+resource.getPath().toString());
//    	 String path=resource.getPath().toString();//.substring(6);
//    	 System.out.println(path);
//    	 FeedRSSParser parser =new FeedRSSParser(path);
//    	 
//
//       try {
//		FeedRssChannel channel=parser.readFeed();
//    	   
//    	   
//	} catch (XMLStreamException e) {
//		// TODO Auto-generated catch block
//		e.printStackTrace();
//	}
   

    }
}

